{
  "name": "pg-source-connector",
  "config": {
    "tasks.max": "1",
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "topic.prefix": "ridehailing",
    "database.hostname": "postgres-source",
    "database.port": "5432",
    "database.user": "user",
    "database.password": "password",
    "database.dbname": "ride_hailing_db",
    "schema.include.list": "public",
    "table.include.list": "public.booking,public.driver_location,public.passenger,public.driver",
    "plugin.name": "pgoutput",
    "publication.autocreate.mode": "filtered",
    "publication.name": "debezium_pub_ridehailing",
    "slot.name": "debezium_slot",
    "slot.drop.on.stop": "false",
    "tombstones.on.delete": "false",
    "include.schema.changes": "false",
    "binary.handling.mode": "base64",
    "time.precision.mode": "adaptive_time_microseconds",
    "heartbeat.interval.ms": "10000",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schemas.enable": "false",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false",
    "decimal.handling.mode": "string",
    "snapshot.mode": "initial",
    "schema.history.internal.kafka.bootstrap.servers": "kafka-1:19092,kafka-2:19092,kafka-3:19092",
    "schema.history.internal.kafka.topic": "schema-changes.ridehailing",
    "topic.creation.default.replication.factor": "3",
    "topic.creation.default.partitions": "3"
  }
}
